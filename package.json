{
	"name": "@natlibfi/melinda-record-import-importer",
	"description": "Record importer for the Melinda record batch import system",
	"author": {
		"name": "The National Library of Finland"
	},
	"keywords": [],
	"homepage": "https://github.com/natlibfi/melinda-record-import-importer",
	"bugs": {
		"url": "https://github.com/natlibfi/melinda-record-import-importer/issues"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:natlibfi/melinda-record-import-importer.git"
	},
	"license": "MIT",
	"version": "1.0.7-alpha.1",
	"main": "./dist/index.js",
	"type": "module",
	"bin": "./dist/cli.js",
	"engines": {
		"node": ">=22"
	},
	"scripts": {
		"start": "node dist/index.js",
		"build": "esbuild \"src/**/*.js\" --sourcemap --platform=node --outdir=dist",
		"cli": "node dist/cli.js",
		"lint": "eslint ./src",
		"lint:dev": "eslint --fix ./src",
		"test:base": "node --test --test-force-exit --experimental-test-coverage --test-reporter=spec './src/*.test.js' './src/**/*.test.js'",
		"test": "npm run lint && npm run test:base",
		"watch:test": "cross-env DEBUG=@natlibfi/* NODE_ENV=test node --watch --test --experimental-test-coverage --test-reporter=spec './src/*.test.js' './src/**/*.test.js'",
		"dev": "npm run watch:test",
		"dev:debug": "cross-env LOG_LEVEL=debug DEBUG=@natlibfi/* NODE_ENV=test npm run watch:test"
	},
	"dependencies": {
		"@natlibfi/marc-record": "^10.0.0-alpha.1",
		"@natlibfi/melinda-backend-commons": "^3.0.0-alpha.6",
		"@natlibfi/melinda-commons": "^14.0.0-alpha.3",
		"@natlibfi/melinda-record-import-commons": "^13.0.0-alpha.5",
		"@natlibfi/melinda-rest-api-client": "^6.0.0-alpha.2",
		"@natlibfi/melinda-rest-api-commons": "^5.0.0-alpha.3",
		"amqplib": "^0.10.3",
		"http-status": "^2.1.0",
		"pretty-print-ms": "^1.0.5"
	},
	"devDependencies": {
		"@natlibfi/fixugen": "^3.0.0-alpha.6",
		"@natlibfi/fixugen-http-client": "^4.0.0-alpha.1",
		"@natlibfi/fixura": "^4.0.0-alpha.13",
		"@natlibfi/fixura-mongo": "^3.0.0-alpha.5",
		"@onify/fake-amqplib": "^3.2.0",
		"cross-env": "^7.0.3",
		"esbuild": "^0.25.9",
		"eslint": "^9.34.0",
		"nock": "^14.0.10"
	},
	"overrides": {
		"nanoid": "^3.3.8"
	}
}